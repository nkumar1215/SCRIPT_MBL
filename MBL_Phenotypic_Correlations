MBL_Yield Traits_Correlations
library(metan)
library(readxl) 
df <- read_xlsx("/Users/nkumar/Desktop/Yield_Traits.xlsx")
df[,1] <- lapply(df[,1], function(x) as.factor(x))
df[,2:14] <- lapply(df [,2:14], function(x) as.numeric(x))
str(df)
corrl <-corr_coef(df)
plot(corrl)
print(corrl)
sink("corrl.txt")
sink()
